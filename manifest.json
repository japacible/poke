{
  "name": "Auto Poking",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Automate poking.",
  "content_scripts": [{
    "matches": [
      "http://*.facebook.com/pokes*",
      "https://*.facebook.com/pokes*"],
    "js": ["jquery.min.js", "main.js"],
    "run_at": "document_end",
    "all_frames": true
  }]
}
